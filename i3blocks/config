separator_block_width=14

[battery]

command=/usr/local/libexec/i3blocks/battery BAT0

interval=5


[disk-root]
label=
command=/usr/local/libexec/i3blocks/disk /
interval=30
color=#1793D1


[wireless]
instance=wlo1
command=/usr/local/libexec/i3blocks/iface
color=#00dd00
interval=10

[volume]
label=vol
command=/usr/local/libexec/i3blocks/volume
interval=2
signal=10
color=#ffa500


[time]
label=
command=date '+%a %d/%m/%y %H:%M'
interval=5
color=#50C878
